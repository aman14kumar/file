#include<stdio.h>
int main()
{
    int a,b,c;
    scanf("%d%d%d",&a,&b,&c);
    if((a==b)&&(a==c)&&(b==c))
        printf("This is an equilateral triangle.");
    else if(((a==b)&&(a!=c))||((b==c)&&(a!=c))||((a==c)&&(a!=b)))
        printf("This is an isoceles triangle.");
    else
        printf("This is a scalene triangle.");
}
